


.clearfix{clear: both;}
/*select box*/
.custom-parent-Box .caret-icon {
    position: absolute;
    right: 25px;
    top: 36%;
    font-size: 11px;
    color: #a1a1a1;
}

/*dashboard page*/

.search {
    width: 236px;
    position: relative;
    display: inline-block;
}
.search .searchTerm {
    height: 40px;
    padding: 6px;
    border-right: 0px;
    background-color: white;
    border: 1px solid #000;
    border-right: none;
}
.search .searchButton {
    position: absolute;
    right: 40px;
    width: 40px;
    height: 40px;
    border: 1px solid #000;
    background: white;
    padding: 5px;
    text-align: center;
    color: #000;
    cursor: pointer;
    font-size: 20px;
    border-left: none;
}

.search-link{
   font-size: 17px;
   text-decoration: underline;
}
.dashboard-wrapper{
    overflow:hidden;
}
.dashboard-column {
    text-align: center;
    padding: 0px;
    min-height: 450px;
}
.dashboard-column h2 {
    width: 100%;
    padding: 10px;
    background-color: #7fc241;
    color: #fff;
    font-size: 20px;
    min-height: 70px;
    font-family: latoregular;
    border-radius: 5px;
}
.dashboard-column ul li a {
    border-radius: 5px;
    font-weight: 18px;
    font-family: latoregular;
}
.dashboard-column ul li a:hover{
    background-color: #fff !important;
    text-decoration: none;
}
.dashboard1 .dashboard-column ul li a {
    background-color: #03a9f4;
    border: 2px solid #03a9f4;
}
.dashboard2 .dashboard-column ul li a {
    background-color: #fd6231;
    border: 2px solid #fd6231;
}
.dashboard3 .dashboard-column ul li a {
    background-color: #9e9e9e;
    border: 2px solid #9e9e9e;
}
.dashboard4 .dashboard-column ul li a {
    background-color: #ffc107;
    border: 2px solid #ffc107;
}
.dashboard5 .dashboard-column ul li a {
    background-color: #1675c3;
    border: 2px solid #1675c3;
}
.dashboard6 .dashboard-column ul li a {
    background-color: #7f4ade;
    border: 2px solid #7f4ade;
}
.dashboard1 .dashboard-column ul li a:hover {
    color: #03a9f4;
    border: 2px solid #03a9f4;
}
.dashboard2 .dashboard-column ul li a:hover{
    color: #fd6231;
    border: 2px solid #fd6231;
}
.dashboard3 .dashboard-column ul li a:hover {
    color: #9e9e9e;
    border: 2px solid #9e9e9e;
}
.dashboard4 .dashboard-column ul li a:hover {
    color: #ffc107;
    border: 2px solid #ffc107;
}
.dashboard5 .dashboard-column ul li a:hover {
    color: #1675c3;
    border: 2px solid #1675c3;
}
.dashboard6 .dashboard-column ul li a:hover {
    color: #7f4ade;
    border: 2px solid #7f4ade;
}
.supervisor-header{
    overflow:hidden ;
}
.supervisor-header h3{
	width: 35%;
    float: left;
    font-family: latomedium;
    padding-top: 10px;
 
}
.supervisor-table {
    width: 100%;
    margin-bottom: 40px;
}
.supervisor-table table{
    background-color: #fff;
}

.supervisor-table table tr th {
    text-align: center;
    vertical-align: middle !important;
    font-family: latobold;
    font-size: 16px;
    background-color: #999;
    color: white;

}
.supervisor-table table tr td {
    vertical-align: middle;
    word-wrap: break-word;
    color: #37414c;
}

.table-width{     /* class for small table*/
    width: 60%;
    margin: 0 auto;
}


/* work in progress*/
.wip-table thead tr th {
    height: 55px;
}  
.supclient{
    width: 18%;
    position: relative;
}
.supclient .select2-container{
    width: 100% !important;
}  
.date-to, .date-from, .session-date{
    width: 10%;
}
.date-to input, .date-from input, .session-date input{
    height: 42px;
    border-radius: 0px;
}
.date-to, .session-date
{
    margin: 0 24px;
}

/*filteration bar*/

.filteration-collapse-panel .panel-title a:focus, .filteration-collapse-panel .panel-title a:hover{
    text-decoration: none !important;
    color:#000 !important;
}
.filteration-collapse-panel .panel-default {
    
    background-color: #fff !important;
}
.filteration-collapse-panel .panel-default>.panel-heading{
    border: 1px solid #7fc241 !important;
    background-color: #fff !important;
    padding: 0;
}
.filteration-collapse-panel .panel-heading{
    position: relative;
}
.filteration-collapse-panel .panel-default>.panel-heading .panel-title a{ 
    display: block; 
    padding: 10px 15px;
}
.filteration-collapse-panel .panel-heading a:after {
font-family: "FontAwesome";
content: "\f056";
position: absolute;
right: 20px;
font-size: 20px;
font-weight: 400;
top: 50%;
line-height: 1;
margin-top: -10px;
}

.filteration-collapse-panel .panel-heading a.collapsed:after {
content: "\f055";
}

.filteration-box{
    padding: 15px;
    position: relative;
}


.ageing-date label, .view label, .receiving-source label{
    margin-bottom: 0px;
    font-size: 16px;
    width: 16%;
    vertical-align: top;
}
.ageing-date label:nth-child(1), .view label:nth-child(1), .receiving-source label:nth-child(1) {
	width: 11%;
    font-size: 16px;
    font-family: latomedium;
    vertical-align: top;
}
.link{
	position: absolute;
    top: 15px;
    right: 15px;
}
.link a{
	font-size: 16px;
    cursor: pointer;
}
/*deleted queue and escalated queue*/

.sup-arsNumber .form-control{
    height: 42px;
    outline: none;
    background-color: #fff;
}
.supervisor-button{
	padding: 9px 25px;
}
/*edit case*/

.edit-case-form-container h4{
	
    font-family: latobold;
    margin: 0px;
}
.edit-case-form .supervisor-form-container{margin-top: 20px; margin-bottom:0px;}
.edit-case-form .supervisor-form-container:nth-child(2),.edit-case-form .supervisor-form-container:nth-child(3), .edit-case-form .supervisor-form-container:nth-child(4) {margin-top: 0px; border: 0px;}
.edit-case-form .supervisor-form-container:nth-child(4){margin-bottom: 20px;}

.supform2{
	border-left: 1px solid #ccc;
}
.sup-form-header{
    padding: 15px 30px;
    border-bottom: 1px solid #eae5e5;
    text-align: left;
}

.form-button{
    padding: 9px 35px;
    border-top: 1px solid #eae5e5;
    overflow:hidden;
}
.supervisor-form{
    padding: 15px 30px;
}
.supervisor-form .form-group{
    position: relative;
}
.supervisor-form .form-group .control-label{ 
    font-family: latomedium;
    font-size: 14px;
} 
.supervisor-form .form-group .form-control{
    width: 100%;
}
.supervisor-form .form-group .form-control, .supervisor-form .form-group .custom-parent-Box{
    height: 42px;
    text-align: left;
}
.supervisor-form .form-group textarea{
    height: 90px !important;
}
.supervisor-form .form-group .custom-parent-Box .select2-container{ 
 	width: 100% !important;
 }
 .supervisor-form-container{
    background-color: #fff;
    border: 1px solid #ccc;
    box-shadow: 1px 1px 5px #9d9d9d;
    border-top: 3px solid #7fc241;
    margin: 50px auto;
 }
 .form-width{    /* class for small form*/
    width: 60%;
    margin: 50px auto;
 } 


 .form-group .radio-inline{
	margin-bottom: 3px;
    margin-right: 10px;
} 

.supervisor-form .phone-number label{
    width: 100%;
}
.supervisor-form .phone-number .custom-parent-Box{
	width: 28%;
    display: inline-block;
    margin-right: 1%;
} 
.supervisor-form .phone-number .std-code{
    width: 18%;
    margin-right: 1%;
    display: inline-block;
}
.supervisor-form .phone-number .mobile-num{
    width: 50%;
    display: inline-block;
}
.supervisor-form .phone-number .addMultipleTel {
    position: absolute;
    top: 32px;
    right: 12px;
    padding: 2px 6px;
    font-size: 20px;
}
.supervisor-form .father-name label{
    width: 100%;
}
.supervisor-form .father-name .form-control{
    display: inline-block;

	width: 31% !important;
	margin-right: 1%;

}

/*close case*/

 .search-ars-button{
    position: absolute;
    right: 16px;
    height: 42px;
    top: 0px;
 }


 /*case history*/
 .case-history-button{
 	text-align: center;
 }
 .case-details .form-group{
    font-size: 15px;
 }
 .case-details .form-group label{
 	font-family: latoregular;
    font-size: 15px;
    margin-bottom: 0px;
    padding-right: 5px;
 }
 .case-details .form-group span{
 	font-family: latobold;
} 



/*set priority*/

.confirmation-message{
	width: 50%;
    margin: 20px auto;
    padding: 9px;
  
}

/*client priority matrix*/
.client-priority-table tbody tr td:nth-child(1){
    text-align: center;
    vertical-align: top;
}
.client-priority-table tbody tr td input{
    width: 30%;
    margin: 0 auto;
    text-align: center;
}



/*pvt queue*/
.searchArs input{
	height: 42px;
    border-radius: 0px;
}
.searchArs{
	margin: 0 24px;
	width: 12%;
}



/*put case on hold table*/

.case-onhold-table tbody tr td input[type="text"]{
	width: 55%;
    margin: 0 auto;
    text-align: center;
}
.table-checkbox{
	text-align: center;
}
.table-checkbox .checkbox-inline{
    display: inline;
}




/*put case onhold form and release from hold form*/
 
.arsDetails .form-group .readable-input{
	height: 30px !important;
	border:none;
}


/*role selector and password change*/

/*time management*/
.agentId{
	width: 10%;
}
.agentId input{
	border-radius: 0px;
	height: 42px;
}


.productivity-header .date-to{
	margin-left: 24px;
	margin-right: 0px;
}



/*real time head cound*/
.real-time-header h3{
	width: 60%;
}



/*tat time summary*/
.time-summary-header .agentId{
	margin-right: 24px;
}

.time-summary-filteration .link{
	position: static;
    margin-bottom: 10px;
}


/*user error report*/
.user-error-header .searchArs{
	margin-right: 0px;
}



/*quality report and field report*/

.quality-report-header .role, .field-report-header .user{
	margin-right: 24px;
}
.role, .user{
	width: 12%;
    position: relative;
}
.role .select2-container, .user .select2-container{
	width: 100% !important;
}


/*sign off pending*/
.edit-link{
	text-align: center;
}
.edit-link a, .edit-link a:focus, .edit-link a:hover{
	text-decoration: underline !important;
    color: #217DD0;
}

/*edit signoff queue page*/
.signoff-status .select2-container{
	width: 188px !important;
}
.changeReason{
	width: 271px;
	border-radius: 0px;
    height: 44px !important;
}

.editSignOffCntr .form-group{margin-bottom: 20px;}
.editSignOffCntr .form-group textarea{width: 100%; height: 150px;}

/*case check status*/
.case-check-container{
	overflow: hidden;
}
.case-check-container .supervisor-table{
	width: 58%;
}
.check-comment textarea{
    width: 60%;
    float: left;
    height: 70px;
}
.check-comment button{
    margin-top: 15px;
    float: right;
}
.additional-checks{
    border: 1px solid #ccc;
    background-color: #fff;
    margin-bottom: 20px;

}
.additional-checks h4{
    padding: 12px;
    border-bottom: 1px solid #ccc;
    text-align: center;
    font-family: latobold;
    color: white;
    background-color: #7fc241;
}
.check-list{
    padding-bottom: 20px;
    font-size: 15px;
    color: #000;
    height: 250px;
}
.check-list ul{
    padding: 20px 60px;
}
.check-list li{
    overflow:hidden;
    padding: 5px 0;
}
.check-list li .add-check{
    float: right;
    color:#7fc241;
    font-size: 18px
}
.check-list li a:hover, .check-list li a:focus{
    color:#7fc241 !important;
}
.config-header h3{
    width: 50%;
}
.configuration-screen-wrapper{
    background-color: #fff;
    padding: 20px 40px;
    box-shadow: 1px 1px 5px #9d9d9d;
    border-top: 3px solid #7fc241;
    margin-bottom: 30px;
}
.configuration-list li label.checkbox-inline{
    padding: 4px 20px;
}
.sub-heading{
    padding-left: 40px;
}
.sub-heading-list{
    padding-left: 30px;
}

.nidCheckBox{
    padding: 12px;
    border: 1px solid #ccc;
}
.check-width{
    width: 35%;
    margin: 0 auto;
    overflow:hidden;
    margin-bottom: 10px;
}
.nidCheckBox ul{
    overflow:hidden;
}
.nidCheckBox ul li{
    margin: 6px 0;
}
.nidCheckBox ul li .check-sort{
    float: right;
    font-size: 16px;
    color: #455262;
}

.check-count{
    width: 30% !important;
    text-align: center;
}
/* Large Screens */

@media only screen and (max-width: 1200px) {
    .dashboard-column h2 {
        font-size: 20px;
    }
    .client-priority-table{
		width: 100%;
	}
	.changeReason {
    	width: 210px;
	}
}
/* Medium Screens */

@media only screen and (max-width: 992px) {
	.supervisor-table{
		overflow-x: auto;
	}
	.supervisor-table table tr th {
    	white-space: nowrap;
	}
    .productivity-table tbody tr:nth-child(1) td, .time-summary-table tbody tr:nth-child(1) td{
        white-space: nowrap;

    }
    .dashboard-column ul li a {
        font-size: 14px;
    }
    .supervisor-header h3 {
    	font-size: 19px;
    }	
    .supclient{
	    width: 22%;
    }
    .non-editable-form .form-group label:nth-child(1), .editable-form .form-group label:nth-child(1) {
        width: 33%;
    }
    .non-editable-form .father-name .form-control, .editable-form .father-name .form-control {
    	width: 19%;
	}
	.form-width{
		width: 80%;
    }
    .client-priority-table tbody tr td input{
    	width: 40%;
    }
    .date-to, .date-from, .agentId, .searchArs, .session-date {
    	width: 15%;
	}
    .table-width{
		width: 100%;
	}
	 
 	.time-summary-header .date-to, .time-summary-header .date-from, .time-summary-header .agentId{
    	width: 11%;
	}
	.quality-report-header h3, .field-report-header h3 {
    	width: 24%;
	}
    .check-list ul {
        padding: 20px 30px;
    }
	
}
/* Tablet Screens */

@media only screen and (max-width: 767px) {
    
    .search {
        width: 220px;
    }
    .search .searchButton {
        right: 40px;
    }
    .form-width{
		width: 100%;
    }
    .case-check-container .supervisor-table {
        width: 100%;
        border:none;
    }
    .case-check-table, .additional-checks{
        margin-bottom: 20px !important;
    }
}
/* Small Screens */

@media only screen and (max-width: 600px) {
    
	.case-details .form-group label{
		width: 100%;
	}
    #contentCntr {
         padding: 0px;
    }	
}
/* Extra Small Screens */

@media only screen and (max-width: 480px) {
	.supervisor-form-heading h2{font-size: 22px;}
}